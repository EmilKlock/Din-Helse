<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".test_results">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="15dp">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nye Prøveresultater:"
            android:textSize="20dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp"
            android:layout_margin="2.5dp"
            android:clickable="true"
            android:background="@drawable/layout_bg">

            <ImageView
                android:id="@+id/imageView11111"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:paddingRight="5dp"
                android:src="@drawable/radiology"
                app:tint="@color/green_2"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView3333"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Røntgen"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView4444"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="27. Mai"
                    android:textSize="20dp" />
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp"
            android:layout_margin="2.5dp"
            android:clickable="true"
            android:background="@drawable/layout_bg">

            <ImageView
                android:id="@+id/imageViewLab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:paddingRight="5dp"
                android:src="@drawable/labs"
                app:tint="@color/green_2"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewLab"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Laboratorietprøver"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textViewLab2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="25. Juni"
                    android:textSize="20dp" />
            </LinearLayout>

        </LinearLayout>
        <TextView
            android:id="@+id/textViewOldRes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:text="Gamle Prøveresultater:"
            android:textSize="20dp"/>

    </LinearLayout>
</FrameLayout>